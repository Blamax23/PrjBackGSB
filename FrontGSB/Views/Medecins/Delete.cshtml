@model ModelGSB.Medecin

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Medecin</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Département")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Departement.NomDep)
        </dd>

        <dt>
            @Html.DisplayName("Nom")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NomMed)
        </dd>

        <dt>
            @Html.DisplayName("Préno")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PrenomMed)
        </dd>

        <dt>
            @Html.DisplayName("Adresse")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AdresseMed)
        </dd>

        <dt>
            @Html.DisplayName("Numéro de téléphone")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TelephoneMed)
        </dd>

        <dt>
            @Html.DisplayName("Spécialité")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SpecialiteComplementaire)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Retour", "Index")
        </div>
    }
</div>
